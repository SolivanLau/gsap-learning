---
import type { IconProps } from "./types";

type Props = IconProps;

const { size, width, height, class: className, ...rest } = Astro.props;
---

<svg
    xmlns="http://www.w3.org/2000/svg"
    width={size || width}
    height={size || height}
    viewBox="0 0 24 24"
    class:list={[className]}
    {...rest}
>
    <path
        fill="none"
        stroke="#ff7214"
        stroke-linecap="round"
        stroke-linejoin="round"
        stroke-width="2"
        d="M5 8h14M5 12h14M5 16h6"></path>
</svg>
