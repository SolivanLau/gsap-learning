---
import type { IconProps } from "@icons/types";

type Props = IconProps;

const { size, width, height, class: className, ...rest } = Astro.props;
---

<svg
    xmlns="http://www.w3.org/2000/svg"
    width={size || width}
    height={size || height}
    viewBox="0 0 24 24"
    class:list={[className]}
    {...rest}
>
    <path
        fill="none"
        stroke="currentColor"
        stroke-linecap="round"
        stroke-linejoin="round"
        stroke-miterlimit="10"
        stroke-width="2"
        d="m21 21l-7.5-7.5M15 9A6 6 0 1 1 3 9a6 6 0 0 1 12 0"></path>
</svg>
